{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "arrow-parens": false,
    "member-access": [
      false
    ]
  },
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false
  }
}
